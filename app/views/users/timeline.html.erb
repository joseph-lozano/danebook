  <main>
    <div class="container">
      <%= render partial: 'shared/cover' %>
      <%= render partial: '/shared/nav_menu' %>

    <section class="timeline-wrap">
      <div class="row">

        <div class="col-md-4">
          <%= render partial: '/timeline_partials/photos_panel' %>
          <%= render partial: '/timeline_partials/friends_panel' %>
        </div> <!--col-md -4 -->
        <div class="col-md-8">
          <% if current_user && current_user == @user  %>
          <%= render partial: '/timeline_partials/new_post' %>
          <% end %>
          <% @posts.each do |p| %>
            <%= render partial: '/timeline_partials/post', :locals => {:p => p} %>
          <% end %>
        </div> <!-- col-md -8 -->
    </section>
  </div> <!-- end of container -->
</main>
