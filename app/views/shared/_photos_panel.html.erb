<div class="panel panel-default friends">
  <div class="panel-heading">
    <h3 class="panel-title timeline-panel-header">Photos <span class="grey-text">(123)</span></h3>
  </div>
  <div class="panel-body">

    <div class="row">
      <div class="col-sm-4">
      <%= current_user.photos.each do |photo| %>
        <%= image_tag photo.picture.url  %>
      </div>
    </div> <!-- end row -->

    <div class="text-center">

      <% if current_user.friends_with?(@user) %>
        
      <%= link_to "See More Photos", user_photos_path(@user), class: 'text-center'  %>
      <% end %>
    </div>

  </div> <!-- end panel -->
</div> <!-- end friends panel-->